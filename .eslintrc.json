{
    "env": {
      "node": true,
      "es6": true,
      "browser": true
    },
    "extends": "airbnb",
    "plugins": [
        "react",
        "jsx-a11y",
        "import"
    ],
    "rules": {
      "semi": [2, "never"],
      "react/jsx-filename-extension": [2, { "extensions": [".js", ".jsx"] }]
    }
}
